From c01a960c51fb9736eb3bb8f7d62d05d206632225 Mon Sep 17 00:00:00 2001
From: Pekka Vuorela <pekka.vuorela@jollamobile.com>
Date: Wed, 11 Oct 2017 16:14:16 +0300
Subject: [PATCH 02/38] Comments out c++11/14 checks

Need autoconf-archive which is not packaged yet
---
 configure.ac | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/configure.ac b/configure.ac
index 291fd448e..bd26f3128 100644
--- a/configure.ac
+++ b/configure.ac
@@ -414,7 +414,7 @@ if test x$enable_aptcc = xyes; then
 	AC_LANG([C++])
 
 	# Require C++11 support
-	AX_CXX_COMPILE_STDCXX_11([ext], [mandatory])
+#	AX_CXX_COMPILE_STDCXX_11([ext], [mandatory])
 
 	# check we have apt and gstreamer headers
 	save_CPPFLAGS="$CPPFLAGS"
@@ -479,7 +479,7 @@ fi
 
 if test x$enable_slack = xyes; then
 	AC_LANG([C++])
-	AX_CXX_COMPILE_STDCXX_14([ext], [mandatory])
+#	AX_CXX_COMPILE_STDCXX_14([ext], [mandatory])
 
 	AC_CHECK_LIB(curl, curl_version, [
 	    CURL_CFLAGS="`curl-config --cflags`"
@@ -506,7 +506,7 @@ if test x$enable_nix = xyes; then
 	AC_LANG([C++])
 
 	# Require C++11 support
-	AX_CXX_COMPILE_STDCXX_11([ext], [mandatory])
+#	AX_CXX_COMPILE_STDCXX_11([ext], [mandatory])
 
 	PKG_CHECK_MODULES(NIX, nix-expr >= 1.12)
 	PKG_CHECK_MODULES(NIX, nix-main >= 1.12)
-- 
